<?xml version="1.0"?>
<CycloneDDS>
  <Domain id="any">
    <SharedMemory>
      <Enable>true</Enable>
      <LogLevel>info</LogLevel>
    </SharedMemory>
    <Compatibility>
      <ManySocketsMode>single</ManySocketsMode>
    </Compatibility>
    <Discovery>
      <EnableTopicDiscoveryEndpoints>false</EnableTopicDiscoveryEndpoints>
    </Discovery>
    <General>
      <AllowMulticast>true</AllowMulticast>
      <EnableMulticastLoopback>true</EnableMulticastLoopback>
      <DontRoute>false</DontRoute>
      <MulticastRecvNetworkInterfaceAddresses>all</MulticastRecvNetworkInterfaceAddresses>
      <RedundantNetworking>true</RedundantNetworking>
      <Interfaces>
        <NetworkInterface autodetermine="true" priority="default" multicast="default"/>
      </Interfaces>
    </General>
  </Domain>
</CycloneDDS>
